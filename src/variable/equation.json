{
  "EQUATION": {
    "01": {
      "template": "\\left\\{\\begin{array}\\\\${0}x${1}y=${2}\\\\${3}x${4}y=${5}\\end{array}\\right.",
      "omitPlus": [0, 2, 3, 5]
    },
    "02": {
      "template": "\\left\\{\\begin{array}\\\\${0}x${1}y${2}z=${3}\\\\${4}x${5}y${6}z=${7}\\\\${8}x${9}y${10}z=${11}\\end{array}\\right.",
      "omitPlus": [0, 3, 4, 7, 8, 11]
    },
