      "template": "\\left\\{\\begin{array}\\\\${0}x${1}y${2}z${3}t=${4}\\\\${5}x${6}y${7}z${8}t=${9}\\\\${10}x${11}y${12}z${13}t=${14}\\\\${15}x${16}y${17}z${18}t=${19}\\end{array}\\right.",
      "omitPlus": [0, 4, 5, 9, 10, 14, 15, 19]
    },
    "04": {
      "template": "${0}x^2${1}x${2}=0",
      "omitPlus": [0]
    },
